{
  "version": 2,
  "buildCommand": "npm install",
  "outputDirectory": ".",
  "public": true,
  "headers": [
    {
      "source": "/editor/config.yml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/x-yaml"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=3600"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}
