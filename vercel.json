{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { 
      "src": "/(.*)\\.(css|js|json|png|jpg|jpeg|svg|ico|woff|woff2)$", 
      "dest": "/$1.$2" 
    },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
